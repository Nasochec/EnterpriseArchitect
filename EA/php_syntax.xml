<SETTINGS>
	<OPTIONS>
		<WordDelimeters>" \t\n,./?LTGT;:\"'{[}]~`%^AMP*()-+=!"</WordDelimeters>
		<SpecialDelimiters>";.{}\n"</SpecialDelimiters>
		<IntelliSenseChars>".&gt;:"</IntelliSenseChars>
	</OPTIONS>
	<COLOR_DATA>
		<KEYWORDS>
			<Color>0000ff</Color>
			<CaseSensitive>False</CaseSensitive>
			<Keyword>if</Keyword>
			<Keyword>elseif</Keyword>
			<Keyword>endif</Keyword>
			<Keyword>else</Keyword>
			<Keyword>continue</Keyword>
			<Keyword>foreach</Keyword>
			<Keyword>endforeach</Keyword>
			<Keyword>for</Keyword>
			<Keyword>as</Keyword>
			<Keyword>define</Keyword>
			<Keyword>endfor</Keyword>
			<Keyword>while</Keyword>
			<Keyword>endwhile</Keyword>
			<Keyword>do</Keyword>
			<Keyword>switch</Keyword>
			<Keyword>case</Keyword>
			<Keyword>default</Keyword>
			<Keyword>endswitch</Keyword>
			<Keyword>break</Keyword>
			<Keyword>function</Keyword>
			<Keyword>cfunction</Keyword>
			<Keyword>return</Keyword>
			<Keyword>exit</Keyword>
			<Keyword>var</Keyword>
			<Keyword>global</Keyword>
			<Keyword>static</Keyword>
			<Keyword>class</Keyword>
			<Keyword>extends</Keyword>
			<Keyword>new</Keyword>
			<Keyword>array</Keyword>
			<Keyword>list</Keyword>
			<Keyword>require</Keyword>
			<Keyword>require_once</Keyword>
			<Keyword>include</Keyword>
			<Keyword>include_once</Keyword>
			<Keyword>echo</Keyword>
			<Keyword>declare</Keyword>
			<Keyword>print</Keyword>
			<Keyword>delete</Keyword>
			<Keyword>true</Keyword>
			<Keyword>false</Keyword>
			<Keyword>or</Keyword>
			<Keyword>and</Keyword>
			<Keyword>xor</Keyword>
			<Keyword>null</Keyword>
			<Keyword>$this</Keyword>
		</KEYWORDS>
		<BLOCKS>
			<BLOCK>
				<Start>/*</Start>
				<End>*/</End>
				<Color>"darkgreen"</Color>
				<WholeText>true</WholeText>
			</BLOCK>
			<BLOCK>
				<Start>"</Start>
				<End>"</End>
				<Color>"darkred"</Color>
				<WholeText>false</WholeText>
			</BLOCK>
			<BLOCK>
				<Start>'</Start>
				<End>'</End>
				<Color>"darkred"</Color>
				<WholeText>false</WholeText>
			</BLOCK>
			<BLOCK>
				<Start>//</Start>
				<Color>"darkgreen"</Color>
				<WholeText>false</WholeText>
			</BLOCK>
		</BLOCKS>
	</COLOR_DATA>
</SETTINGS>